{
    "name": "arniro/laravel-sluggable",
    "description": "A package to generate a slug for Laravel models. Translatable columns are also supported.",
    "license": "MIT",
    "version": "0.1.2",
    "authors": [
        {
            "name": "Artyom Nishchuk",
            "email": "artyom@arniro.io",
            "homepage": "https://arniro.io"
        }
    ],
    "homepage": "https://github.com/arniro/laravel-sluggable",
    "keywords": ["Laravel", "Sluggable", "Translatable"],
    "require": {
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0",
        "spatie/laravel-translatable": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Arniro\\Sluggable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arniro\\Sluggable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arniro\\Sluggable\\SluggableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
